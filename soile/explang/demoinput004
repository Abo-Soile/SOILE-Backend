gvar gv1 <- imagefile("url1")
val vl1 <- null

function fx2(x1 x2)
  var v2 <- vl1

  hide("moi")
  show({a:"str" b:12 c:[1 2 3 4] d:{a:"d1" b:"d2"}})
end

function fx(x1 x2)
  fvar fv1 <- gv1
  var v2 <- vl1

  hide("moi")
  wait(1000)
  vl1 <- {a:"str" b:12 c:[1 2 3 4] d:{a:"d1" b:"d2"}}
  fx(vl1 v2)
  fx2({a:"str" b:12 c:[1 2 3 4] d:{a:"d1" b:"d2"}})
end

iteration
  hide("moi")
